




{
   "forwardingGateway": {
      "requiredHeader": "X-Forwarded-Host",
      "allowedForwardedHosts": [
        "tamimelseisy.com",
        "www.tamimelseisy.com"
      ]
    },
  
  "auth": {
   
    "identityProviders": {
      "customOpenIdConnectProviders": {
        "employees": {
          "registration": {
            "clientIdSettingName": "AAD_CLIENT_ID",
            "clientCredential": {
              "clientSecretSettingName": "AAD_CLIENT_SECRET"
            },
            "openIdConnectConfiguration": {
              "wellKnownOpenIdConfiguration": "https://login.microsoftonline.com/d7b3fbbf-00fd-43ab-a8e6-a3d37ac54a72/v2.0/.well-known/openid-configuration"
            }
          },
          "login": {
            "scopes": [
              "openid",
              "profile",
              "email"
            ]
          }
        },
        "aadb2c": {
          "registration": {
            "clientIdSettingName": "AADB2C_CLIENT_ID",
            "clientCredential": {
              "clientSecretSettingName": "AADB2C_CLIENT_SECRET"
            },
            "openIdConnectConfiguration": {
              "wellKnownOpenIdConfiguration": "https://tamimcustomers.ciamlogin.com/e89cfffb-6bfc-4fc5-966c-9237eab1fd1d/v2.0/.well-known/openid-configuration?appid=91539a1d-69df-4087-a533-691b2a0763fe"
            }
          },
          "login": {
            "scopes": [
              "openid",
              "profile",
              "email"
            ]
          }
        }
      }
    }
  },
  "routes": [
    {
      "route": "/login-employees",
      "redirect": "/.auth/login/employees",
      "statusCode": 302
    },
    {
      "route": "/login-customers",
      "redirect": "/.auth/login/aadb2c",
      "statusCode": 302
    },
    {
      "route": "/employees/*",
      "allowedRoles": [
        "authenticated"
      ]
    },
    {
      "route": "/customers/*",
      "allowedRoles": [
        "authenticated"
      ]
    },
    {
      "route": "/logout",
      "redirect": "/.auth/logout",
      "statusCode": 302
    },
    {
      "route": "/*",
      "allowedRoles": [
        "anonymous",
        "authenticated"
      ]
    }
  ],
  "responseOverrides": {
    "401": {
      "redirect": "/login-customers",
      "statusCode": 302
    }
  }
}